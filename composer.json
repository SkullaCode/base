{
    "name": "skullacode/baseline",
    "description": "basline for PHP projects",
    "license": "",
    "type": "project",
    "authors": [
        {
            "email" : "rodney@axium.io",
            "name": "Rodney Forbes",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "monolog/monolog": "^1.17",
        "catfan/medoo": "^1.5",
        "league/flysystem": "^1.0",
        "phpmailer/phpmailer": "^6.0",
        "respect/validation": "^1.1",
        "robmorgan/phinx": "^0.10.1",
        "bryanjhv/slim-session": "^3.6",
        "ext-json": "*",
        "ext-pdo": "*",
        "guzzlehttp/guzzle": "^6.3",
        "webonyx/graphql-php": "^0.13.5"
    },
    "autoload": {
        "psr-4": {
            "Software\\": "software/",
            "App\\": "app/",
            "App\\Constant\\": "app/constants/",
            "App\\Controller\\": "app/controllers/",
            "App\\MiddleWare\\": "app/middlewares/",
            "App\\Utility\\": "app/utilities/",
            "App\\Model\\": "app/models/",
            "App\\ViewModel\\": "app/viewmodels/",
            "App\\DBContext\\": "app/contexts/",
            "App\\Extension\\": "app/extensions/",
            "App\\Traits\\": "app/traits/",
            "App\\Interfaces\\": "app/interfaces/",
            "App\\Provider\\": "app/providers/",
            "Development\\DBContext\\": "dev/contexts/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "serve": "php -S localhost:8137 -t public public/index.php"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.7"
    }
}
